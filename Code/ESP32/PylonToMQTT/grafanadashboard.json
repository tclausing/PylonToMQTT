            "query": "import \"strings\"\r\n\r\nvalues = from(bucket: \"pylontomqtt\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r._field == \"CellVoltage\" and r.pack == \"$pack\")\r\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\r\n  |> map(fn: (r) => ({ \r\n      _field: if strings.strlen(v: r.cell) > 1 then r.cell else \"0${r.cell}\", \r\n      _time: r._time, _value: r._value }))\r\n  |> yield(name: \"mean\")\r\n\r\n//values\r\n//  |> map(fn: (r) => ({ r with _field: \"spread\" }))\r\n//  |> aggregateWindow(every: v.windowPeriod, fn: spread, createEmpty: false)\r\n//  |> yield(name: \"spread\")",
